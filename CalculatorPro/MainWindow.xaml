<Window x:Class="CalculatorPro.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculatorPro"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Name="factor1TextBox" Margin="60,130,400,130"></TextBox>
        <TextBox Name="factor2TextBox" Margin="130,130,330,130"></TextBox>
        <TextBox Name="resultTextBox" Margin="200,130,260,130"></TextBox>
        <Button Name="calculateButton" Margin="200,200,100,50" Click="calculateButton_Click">Oblicz</Button>
    </Grid>
</Window>
